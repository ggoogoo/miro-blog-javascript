    <script>

    var randomstring = '';

    function randomString(){
        randomstring = '';
        var chars = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXTZabcdefghiklmnopqrstuvwxyz";
        var string_length = 8;
        var p = new Promise(function(resolve, reject){
          for (var i=0; i<string_length; i++){
            var rnum = Math.floor(Math.random() * chars.length);
            randomstring += chars.substring(rnum,rnum+1);
            if(i == 7){
              resolve(randomstring);
            }
          }
        })
        p.then(function(randomstring){
          console.log(randomstring);
          document.getElementById('randomstring').innerHTML= randomstring;
          document.getElementById("1").value = randomstring;
        })
    }

    function answerCheck(){
        document.getElementById("2").value = document.getElementById("answer").value;
        ajax();
    }

    function ajax(){
      axios.post('/Attendance', {
        randomstring: randomstring,
        answer: document.getElementById("2").value
      });
    }

    function over(){
      document.getElementById('randomstring').innerHTML = "출석이 끝났습니다.";
      ajax2();
    }

    function ajax2(){
      axios.post('/Attendance', {
        randomstring: document.getElementById('randomstring').innerHTML
      });
    }

    </script>

